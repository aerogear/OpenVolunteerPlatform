{
  "name": "openvp",
  "version": "0.5.1",
  "description": "Command line tool for Open Voluteer Platform",
  "main": "dist/index.js",
  "bin": {
    "openvp": "dist/index.js"
  },
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc",
    "cmdinit": "ts-node ./dist/index.js init test",
    "watch": "tsc -w",
    "lint": "eslint -t stylish --project \"tsconfig.json\"",
    "test": "jest"
  },
  "engines": {
    "node": "20.18.0"
  },
  "devDependencies": {
    "@types/chalk": "2.2.4",
    "@types/inquirer": "9.0.7",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.17.0",
    "@types/node-emoji": "2.1.0",
    "@types/through": "0.0.30",
    "@types/yargs": "17.0.33",
    "jest": "26.6.3",
    "lint-staged": "15.2.10",
    "ncp": "2.0.0",
    "rimraf": "6.0.1",
    "ts-jest": "25.5.1",
    "ts-node": "10.9.2",
    "tsutils": "3.17.1",
    "typescript": "5.6.3",
    "typescript-eslint-parser": "22.0.0"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "chokidar": "4.0.1",
    "debounce": "2.2.0",
    "execa": "9.4.1",
    "figlet": "1.5.0",
    "glob": "11.0.0",
    "inquirer": "12.0.0",
    "node-emoji": "2.1.3",
    "ora": "8.1.0",
    "parse-github-url": "1.0.2",
    "request": "2.88.2",
    "tar": "6.0.5",
    "tmp": "0.2.1",
    "yargs": "17.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
