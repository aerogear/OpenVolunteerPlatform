{
  "name": "openvp",
  "version": "0.5.1",
  "description": "Command line tool for Open Voluteer Platform",
  "main": "dist/index.js",
  "bin": {
    "openvp": "dist/index.js"
  },
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc",
    "cmdinit": "ts-node ./dist/index.js init test",
    "watch": "tsc -w",
    "lint": "eslint -t stylish --project \"tsconfig.json\"",
    "test": "jest"
  },
  "engines": {
    "node": "14.21.3"
  },
  "devDependencies": {
    "@types/chalk": "0.4.31",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.2.3",
    "@types/js-yaml": "3.12.10",
    "@types/node": "12.20.55",
    "@types/node-emoji": "1.8.2",
    "@types/through": "0.0.33",
    "@types/yargs": "15.0.19",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "ncp": "2.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.2",
    "tsutils": "3.21.0",
    "typescript": "3.9.10",
    "typescript-eslint-parser": "21.0.2"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "chokidar": "3.6.0",
    "debounce": "1.2.1",
    "execa": "4.1.0",
    "figlet": "1.8.0",
    "glob": "7.2.3",
    "inquirer": "7.3.3",
    "node-emoji": "1.11.0",
    "ora": "3.4.0",
    "parse-github-url": "1.0.3",
    "request": "2.88.2",
    "tar": "6.0.5",
    "tmp": "0.2.3",
    "yargs": "15.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
